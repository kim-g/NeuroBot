<UserControl x:Class="NeuroBot.bot"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NeuroBot"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Width="10" Height="10">
    <Grid MouseUp="Grid_MouseUp">
        <Rectangle x:Name="Rect" Fill="#FFFFA2A2" Stroke="#FFF5F507"/>
        <Rectangle x:Name="eye" Fill="Black" Margin="6,1,2,7"/>
        <Rectangle x:Name="eye2" Fill="Black" Margin="2,1,6,7"/>


    </Grid>
</UserControl>
