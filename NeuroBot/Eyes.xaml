        <TextBox x:Name="ColorR" Margin="195,133,522,258" Text="255" TextChanged="Color_TextChanged"/>
        <TextBox x:Name="ColorG" Margin="289,133,428,258" Text="255" TextChanged="Color_TextChanged"/>
